{
  "manifest_version": 3,
  "name": "DemoAPP",
  "version": "1.0",
  "description": "DemoAPP",
  "permissions": [
    "clipboardRead",
    "clipboardWrite"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_start",
      "all_frames": true
    }
  ]
}